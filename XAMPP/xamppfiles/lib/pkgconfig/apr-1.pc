prefix=/Applications/XAMPP/xamppfiles
exec_prefix=/Applications/XAMPP/xamppfiles
libdir=${prefix}/lib
APR_MAJOR_VERSION=1
includedir=/Applications/XAMPP/xamppfiles/include

Name: APR
Description: The Apache Portable Runtime library
Version: 1.3.9
Libs: -L${libdir} -lapr-${APR_MAJOR_VERSION} -lpthread
Cflags:  -DDARWIN -DSIGPROCMASK_SETS_THREAD_MASK -no-cpp-precomp  -I${includedir}
